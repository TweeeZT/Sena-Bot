# Discord Bot Configuration
DISCORD_TOKEN=your_discord_bot_token_here
BOT_TOKEN=your_discord_bot_token_here
CLIENT_ID=your_discord_application_id_here

# Optional: Message content intent (set to true if needed)
ENABLE_MESSAGE_CONTENT=true

# osu! API Configuration
OSU_CLIENT_ID=your_osu_client_id
OSU_CLIENT_SECRET=your_osu_client_secret
OSU_USERNAME=your_osu_username
OSU_USER_ID=your_osu_user_id
OSU_FEED_CHANNEL_ID=discord_channel_id_for_osu_feed
OSU_RECENT_CHANNEL_ID=discord_channel_id_for_recent_plays
OSU_RECENT_POLL_SECONDS=60

# Valorant API Configuration (HenrikDev API)
VALORANT_NAME=your_valorant_name
VALORANT_TAG=your_valorant_tag
VALORANT_REGION=ap
HENRIK_API_KEY=your_henrikdev_api_key
VALORANT_API_KEY=your_henrikdev_api_key

# Optional: Tracker.gg API (alternative)
TRACKER_GG_API_KEY=your_tracker_gg_api_key
VALORANT_API_PROVIDER=tracker

# Music Player Configuration
MAX_PLAYLIST_LENGTH=400

# Optional: Bot Presence/Status
# BOT_STATUS_TEXT=with /play and /help
# BOT_STATUS_TYPE=PLAYING
# BOT_STATUS_STATE=online
# BOT_STATUS_STREAM_URL=https://twitch.tv/yourchannel
